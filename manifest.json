{
  "manifest_version": 2,
  "name": "FB - Sponsorless",
  "version": "2.0",
  "description": "A extension to remove facebook sponsored posts.",
  "icons": {
    "16": "icons/fb-ads-16.png",
    "32": "icons/fb-ads-32.png",
    "48": "icons/fb-ads-48.png",
    "64": "icons/fb-ads-64.png",
    "96": "icons/fb-ads-96.png",
    "128": "icons/fb-ads-128.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["*://*.facebook.com/*"]
    }
  ]
}
