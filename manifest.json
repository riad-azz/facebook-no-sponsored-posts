{
  "manifest_version": 2,
  "name": "FB - Sponserless",
  "version": "1.0",
  "description": "A extension to remove facebook sponsored posts.",
  "icons": {
    "48": "icons/fb-ads-48.png"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*:\/\/*.facebook.com/*"
      ]
    }
  ]
}
